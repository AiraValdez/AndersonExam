@using AndersonExamModel;
@model Examinee


@{
    ViewBag.Title = "SelectExam";
}

<h2>Select Exam</h2>
<div class="panel panel-default">
    <div class="panel-heading">Exams</div>
    <div class="panel-body">
        <div ng-controller="ExamineeController as model" ng-init="model.Initialise(@Model.ExamineeId)">
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-hover table-sm table-bordered">
                        <thead>
                            <tr class="thead-inverse">
                                <th>Exam Name</th>
                                <th>Exam Id</th>
                                <th>Time Limit</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>